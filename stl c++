#include <iostream>
#include <bits/stdc++.h>
#include <array>
using namespace std;
int main()
{
    cout << "Hello world" << endl;
    array<int, 5> arr = {3, 4, 5, 5, 6};
    for (auto it : arr)
    {
        cout << it << " ";
    }
    cout << endl;
    cout << arr.size() << endl;
    cout << arr.front() << endl;
    cout << arr.back() << endl;
    cout << arr.at(2) << endl;
    string s = "nkjgdiugvbkj";
    for (auto c : s)
    {
        cout << c;
    }
    cout << endl;
    vector<int> a;
    cout << a.size() << endl;
    a.push_back(2);
    a.push_back(3);
    cout << a.size() << endl;
    for (int i = 0; i < a.size(); i++)
    {
        cout << a[i] << " ";
    }
    cout << endl;
    vector<int> vec1(5, 10);
    for (int i = 0; i < vec1.size(); i++)
    {
        cout << vec1[i] << " ";
    }
    cout << endl;
    vector<int> vec2(vec1.begin(), vec1.begin() + 3);
    for (int i = 0; i < vec2.size(); i++)
    {
        cout << vec2[i] << " ";
    }
    vec1.clear();
    vec1.push_back(2);
    for (int i = 0; i < vec1.size(); i++)
    {
        cout << vec1[i] << " ";
    }
    vector<vector<int>> vec;
    vector<int> r1;
    r1.push_back(1);
    r1.push_back(2);
    r1.push_back(3);
    vector<int> r2;
    r2.push_back(10);
    r2.push_back(20);

    vec.push_back(r1);
    vec.push_back(r2);

    for (auto vctr : vec)
    {
        for (auto it : vctr)
        {
            cout << it << " ";
        }
        cout << endl;
    }
    for (int i = 0; i < vec.size(); i++)
    {
        for (int j = 0; j < vec[i].size(); j++)
        {
            cout << vec[i][j] << " ";
        }
        cout << endl;
    }
    define 10*20 vector
    vector<vector<int>> vec(10, vector<int>(20, 0));
    for (int i = 0; i < vec.size(); i++)
    {
        for (int j = 0; j < vec[i].size(); j++)
        {
            cout << vec[i][j] << " ";
        }
        cout << endl;
    }
    // storing array in vector
    vector<int> arr[4];
    3d vectors
    10*20*30
    vector<vector<vector<int>>> vec(10, vector<vector<int>>(20, vector<int>(30, 0)));

}
