pointer arithmetic:
#include<stdio.h>
void main()
{
    int i=56;
    int *ptr1 = &i ;
    printf("value of i is %u \n",ptr1);
    ptr1++;
     printf("value of i is %u \n",ptr1);
    
     char a=78;
     char ptr2=&a;
     printf("value of a is %u \n",ptr2);
     ptr2++;
     printf("value of a is %u \n",ptr2);
     }
     $$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$
     arrays in pointers:
     #include<stdio.h>
void main()
{
    int marks[5];
    int *ptr=&marks[0];
    for(int i=0;i<5;i++)
    {
        printf("marks of %dth student : \n",i+1);
        scanf("%d",ptr);
        ptr++;
    }
    for(int i=0;i<5;i++)
    {
       printf("marks of %dth student : %d \n", i+1,marks[i]);
    }
}
*******************************************************************************************************************************************************************
arrays in functions :
#include<stdio.h>
void printarray(int *ptr,int n)
{ for(int i=0;i<n;i++)
    printf("value of element %d is %d\n",i+1,*(ptr+i));

}
int main()
{
    int marks[]={46,45,66,77,78,44,89};
    printarray(marks,7);
    return 0;
}
