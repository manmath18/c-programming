strings in c:

#include <stdio.h>
#include<string.h>
int main()
{
    char s1[45],count;
    printf("enter name : ");
    scanf("%s",s1);
    puts(s1);
    count=strlen(s1);
    printf("%d \n",count);

    return 0;
}
###################################################################################################################################33
#include <stdio.h>
#include<string.h>
int main()
{
    char s1[45],count=0;
    printf("enter name : ");
    scanf("%s",s1);
    puts(s1);
    int i=0;
   while(s1[i]!='\0'){
       count++;
       i++;
       
   }
   printf("%d\n",count);

    return 0;
}
*************************************************************************************************************************************************************************

#include <stdio.h>
#include<string.h>
int main()
{
    char s1[8]="Manmath";
    char s2[9]="Mungde";
    int l1,l2;
    l1=strlen(s1);
    l2=strlen(s2);
    for(int i=0;i<l2;i++){
        s1[l1+i]=s2[i];
    }
    printf("%s",s1);
    return 0;
}
