#include<stdio.h>
int main()
{
    int marks[5];
    for(int i=0;i<5;i++)
    {
    printf("enter marks of student %d: ",i+1);

    scanf("%d",&marks[i]);
    }
  

printf("you have entered marks as %d ,%d,%d and %d",marks[0],marks[1],marks[2],marks[3]);


return 0;



}
