multiplication table :
#include<stdio.h>
int main()
{
    int mul[10];
    for(int i=0;i<10;i++)
    {
       mul[i]=5*(i+1);
    }
 for(int i=0;i<10;i++)
 {
    printf(" 5* %d= %d\n",i+1,mul[i]);
 }
    
    
      
    return 0;
}
###################################################################################################################################################################
reverse array;
#include<stdio.h>
void reverse(int *arr,int n)
{
    int temp;
  for(int i=0;i<3;i++)
    {
        temp =arr[i];
        arr[i]=arr[n-i-1];
        arr[n-i-1]=temp;
    }
}
int main()
{
    int arr[7]={1,2,3,4,5,6,7};
    reverse(arr,7);
      for(int i=0;i<7;i++){
 printf("%d th element is %d\n",i+1,arr[i]);
      }
return 0;
}
%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
positive numbers in array:
#include<stdio.h>
int main()
{    int p=0,n=0;
    int arr[9]={5,-9,6,9,-6,2,-8,6,4};

    for(int i=0;i<9;i++)
    {
        if(arr[i]>0)
        { p++;}
        else 
         {n++;}

    }
    printf("no. of positive integers is %d \n",p);
}
